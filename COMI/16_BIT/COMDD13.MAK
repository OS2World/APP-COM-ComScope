PROJ = COMDD13
PROJFILE = COMDD13.MAK
DEBUG = 1

PWBRMAKE  = pwbrmake
NMAKEBSC1  = set
NMAKEBSC2  = nmake
BROWSE  = 0
BREXCLUDE  = i:\include\os2.h i:\masm\include\os2.inc\
        i:\masm\include\bseerr.inc I:\MASM\INCLUDE\BSEDEV.INC\
        I:\MASM\INCLUDE\BSEDOS.INC I:\MASM\INCLUDE\BSEERR.INC\
