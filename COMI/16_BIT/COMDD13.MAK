        I:\INCLUDE\DOS.H
CFLAGS_G  = /Asnw /W3 /G2 /Dx16_BIT /Zp /BATCH /nologo
AFLAGS_G  = /Cp /W2 /Dx16_BIT /nologo /Zp1 /Sg
CC  = cl
CFLAGS_D  = /Od /Gs
CFLAGS_R  = /Od /Gs
ASM  = ml
AFLAGS_D  = /Sn /Zf /Fl
AFLAGS_R  = /Sn /Zf
MAPFILE_D  = 16_BIT\$(PROJ).map
MAPFILE_R  = NUL
LFLAGS_G  =  /NOI /NOE /NOD /STACK:0  /BATCH /nologo
LFLAGS_D  =  /M /NOF /NOP
LFLAGS_R  =  /NOF /NOP
LINKER  = link
ILINK  = ilink
LRF  = echo > NUL
LLIBS_G  = w:\toolkt13\doscalls.lib
CVFLAGS  =  /50 /C"n16"
NMFLAGS  = /nologo
BIND  = bind

DEF_FILE  = COMDD13.DEF
OBJS  = 16_BIT\DATA_SEG.obj 16_BIT\UTILITY.obj 16_BIT\COMDD.obj\
        16_BIT\IOCTL.obj 16_BIT\INT.obj 16_BIT\UTIL.obj\
        16_BIT\ProcessFlags.obj 16_BIT\INIT.obj 16_BIT\HDW_TEST.obj\
