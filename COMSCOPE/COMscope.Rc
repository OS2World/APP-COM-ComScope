MENU IDM_COMSCOPE
BEGIN

    SUBMENU "~File",                   IDM_FILE, MIS_TEXT
    BEGIN
        MENUITEM "~Load Data",         IDM_LOADDAT, MIS_TEXT
        MENUITEM "~Save Data",         IDM_SAVEDAT, MIS_TEXT
        MENUITEM "Save Data ~As...",   IDM_SAVEDATAS, MIS_TEXT
        MENUITEM SEPARATOR
        MENUITEM "E~xit \tF3",         IDM_EXIT, MIS_TEXT
    END

    SUBMENU "~Action",                 IDM_ACTION, MIS_TEXT
    BEGIN
        MENUITEM "~Capture Stream \tF5", IDM_MSTREAM, MIS_TEXT
        MENUITEM "~Display Stream \tF6", IDM_MDISPLAY, MIS_TEXT
        MENUITEM "~Examine Stream \tF4", IDM_VIEWDAT, MIS_TEXT
        MENUITEM SEPARATOR
        MENUITEM "~Search Stream \tAlt-F2", IDM_SEARCH, MIS_TEXT
        MENUITEM "Find ~Next \tF2",    IDM_SEARCH_NEXT, MIS_TEXT
        MENUITEM "~Query Event \tCtl-F2", IDM_QUERY_INDEX, MIS_TEXT
        MENUITEM SEPARATOR

        SUBMENU "~Surface Sessions",   IDM_SURFACE, MIS_TEXT
        BEGIN
            MENUITEM "~This Session \tF11", IDM_SURFACE_THIS, MIS_TEXT
            MENUITEM "~All Sessions \tF12", IDM_SURFACE_ALL, MIS_TEXT
        END
    END

    SUBMENU "~Capture",                IDM_TRIGGER, MIS_TEXT
    BEGIN
        MENUITEM "~Events...",         IDM_CAP_EVENT, MIS_TEXT
        MENUITEM "~Process...",        IDM_BUFFSIZE, MIS_TEXT
        MENUITEM "~Update Frequency...", IDM_MSLEEP, MIS_TEXT

        SUBMENU "~Display",            IDM_DISPLAY_OPT, MIS_TEXT
        BEGIN

            SUBMENU "Stream ~Display Format\tF8 ", IDM_DISP_TYPE, MIS_TEXT
            BEGIN
                MENUITEM "~Lexical",   IDM_COLUMNS, MIS_TEXT
                MENUITEM "~Time-relational", IDM_ROWS, MIS_TEXT
            END
            MENUITEM "Toggle ~Font Size \tF10", IDM_TOGGLE_FONT_SIZE, MIS_TEXT
            MENUITEM SEPARATOR

            SUBMENU "~Time-relational", IDM_ROW_DISPLAY, MIS_TEXT
            BEGIN
                MENUITEM "~Setup...",  IDMPU_EVENT_DISP, MIS_TEXT

                SUBMENU "~Colors",     IDMPU_COLORS, MIS_TEXT
                BEGIN
                    MENUITEM "~Receive...", IDMPU_READ_COLOR, MIS_TEXT
                    MENUITEM "~Transmit...", IDMPU_WRITE_COLOR, MIS_TEXT
                    MENUITEM "Modem ~in...", IDMPU_MODEMIN_COLOR, MIS_TEXT
                    MENUITEM "Modem ~out...", IDMPU_MODEMOUT_COLOR, MIS_TEXT
                    MENUITEM "Open/~Close...", IDMPU_OPEN_COLOR, MIS_TEXT
                    MENUITEM "~Write requests...", IDMPU_WRITEREQ_COLOR,
                             MIS_TEXT
                    MENUITEM "Read Re~quests...", IDMPU_READREQ_COLOR, MIS_TEXT
                    MENUITEM "~Device I/O Control calls...",
                              IDMPU_DEVIOCTL_COLOR, MIS_TEXT
                    MENUITEM "Communications ~Errors...", IDMPU_ERROR_COLOR,
                             MIS_TEXT
                END

                SUBMENU "~Font\tF9 ",  IDMPU_FONT, MIS_TEXT
                BEGIN
                    MENUITEM "~ASCII", IDMPU_ASCII_FONT, MIS_TEXT
                    MENUITEM "~Hexidecimal", IDMPU_HEX_FONT, MIS_TEXT
                    MENUITEM "~ASCII", IDMPU_LEX_ASCII_FONT, MIS_TEXT
                    MENUITEM "~Hexidecimal", IDMPU_LEX_HEX_FONT, MIS_TEXT
                END
            END

            SUBMENU "~Lexical",        IDM_COL_DISPLAY, MIS_TEXT
            BEGIN
                MENUITEM "~Sync receive", IDMPU_SYNC_RX, MIS_TEXT
                MENUITEM "S~ync transmit", IDMPU_SYNC_TX, MIS_TEXT
                MENUITEM "Setup ~receive...", IDMPU_RX_DISP_FILTERS, MIS_TEXT
                MENUITEM "Setup ~transmit...", IDMPU_TX_DISP_FILTERS, MIS_TEXT
                MENUITEM "Receive ~colors...", IDMPU_RX_COLORS, MIS_TEXT
                MENUITEM "Transmit co~lors...", IDMPU_TX_COLORS, MIS_TEXT
                MENUITEM "~Lock receive width", IDMPU_LOCK_WIDTH, MIS_TEXT
                MENUITEM "Lock transmit ~width", IDMPU_LOCK_WIDTH, MIS_TEXT

                SUBMENU "~Font\tF9 ",  IDMPU_FONT, MIS_TEXT
                BEGIN
                    MENUITEM "~ASCII", IDMPU_ASCII_FONT, MIS_TEXT
                    MENUITEM "~Hexidecimal", IDMPU_HEX_FONT, MIS_TEXT
                    MENUITEM "~ASCII", IDMPU_LEX_ASCII_FONT, MIS_TEXT
                    MENUITEM "~Hexidecimal", IDMPU_LEX_HEX_FONT, MIS_TEXT
                END
            END

            SUBMENU "~Status Bar",     IDM_STATUS_DISPLAY, MIS_TEXT
            BEGIN
                MENUITEM "Show Last ~Message", IDMPU_LAST_MSG, MIS_TEXT
                MENUITEM "~Colors...", IDMPU_COLORS, MIS_TEXT
                MENUITEM "Show Captured Co~unts", IDMPU_LAST_COUNT, MIS_TEXT
                MENUITEM "~Silent Status", IDMPU_SILENT_STATUS, MIS_TEXT
            END
        END
    END

    SUBMENU "~Options",                IDM_OPTION, MIS_TEXT
    BEGIN
        MENUITEM "Stic~ky Pop-up Menus", IDM_STICKY_MENUS, MIS_TEXT
        MENUITEM "~Silent Status \tF7", IDM_SILENT_STATUS, MIS_TEXT
        MENUITEM SEPARATOR
        MENUITEM "~Manage Profile...", IDM_MANAGE_CFG, MIS_TEXT
    END

    SUBMENU "~Device",                 IDM_DEVICE, MIS_TEXT
    BEGIN
        MENUITEM "Select ~Device...",  IDM_SSELECT, MIS_TEXT
        MENUITEM "~Install Device...", IDM_INSTALL, MIS_TEXT

        SUBMENU "~Status",             IDM_STATUS, MIS_TEXT
        BEGIN
            MENUITEM "~Device Driver Status", IDM_STDEVDRV, MIS_TEXT
            MENUITEM "~Input MODEM Signals", IDM_STINMDM, MIS_TEXT
            MENUITEM "~Output MODEM Signals", IDM_STOUTMDM, MIS_TEXT
            MENUITEM "~Receive Buffer Status", IDM_STRCVBUFF, MIS_TEXT
            MENUITEM "~Transmit Buffer Status", IDM_STXMITBUF, MIS_TEXT
            MENUITEM "Character ~Counts...", IDM_SHOWCOUNTS, MIS_TEXT
            MENUITEM SEPARATOR
            MENUITEM "Set ~Update Frequency...", IDM_STUPDATE, MIS_TEXT
            MENUITEM SEPARATOR
            MENUITEM "Show ~All...",   IDM_STALL, MIS_TEXT
        END

        SUBMENU "~Control",            IDM_IOCTL, MIS_TEXT
        BEGIN
            MENUITEM "Transmit Next ~Keystroke", IDM_SENDIMM, MIS_TEXT

            SUBMENU "~Force Xon/Xoff", IDM_FORCEXX, MIS_TEXT
            BEGIN
                MENUITEM "Act Like Xo~n Received", IDM_FXON, MIS_TEXT
                MENUITEM "Act Like Xo~ff Received", IDM_FXOFF, MIS_TEXT
            END

            SUBMENU "Transmit ~Xon/Xoff Now!", IDM_SENDXX, MIS_TEXT
            BEGIN
                MENUITEM "Send Xo~n",  IDM_SXON, MIS_TEXT
                MENUITEM "Send Xo~ff", IDM_SXOFF, MIS_TEXT
            END
            MENUITEM "Transmit ~Break", IDM_SENDBRK, MIS_TEXT

            SUBMENU "~Flush Buffers",  IDM_FLUSH, MIS_TEXT
            BEGIN
                MENUITEM "Flush ~Input", IDM_FINPUT, MIS_TEXT
                MENUITEM "Flush ~Output", IDM_FOUTPUT, MIS_TEXT
                MENUITEM "Flush ~Both", IDM_FBOTH, MIS_TEXT
            END
            MENUITEM "Output Modem Signals...", IDM_OUT1, MIS_TEXT
        END

        SUBMENU "~Protocol",           IDM_SETUP, MIS_TEXT
        BEGIN
            MENUITEM "~Baud Rate...",  IDM_BAUDRATE, MIS_TEXT
            MENUITEM "~Line Protocol...", IDM_PROTO, MIS_TEXT
            MENUITEM "~Time-outs...",  IDM_TIMEOUT, MIS_TEXT
            MENUITEM "~Handshaking...", IDM_HANDSHAKE, MIS_TEXT
            MENUITEM "~Stream Filters...", IDM_FILTER, MIS_TEXT
            MENUITEM "~FIFO Control...", IDM_FIFO, MIS_TEXT
        END
    END

    SUBMENU "~Help",                   IDM_HELP, MIS_TEXT
    BEGIN
        MENUITEM "Help ~Index",        IDM_HELPINDEX, MIS_TEXT
        MENUITEM "~General Help",      IDM_HELPEXTEND, MIS_TEXT
        MENUITEM "~Keys Help",         IDM_HELPKEYS, MIS_TEXT
        MENUITEM "~Using Help",        IDM_HELPFORHELP, MIS_TEXT
        MENUITEM SEPARATOR
        MENUITEM "~About COMscope",    IDM_HELPABOUT, MIS_TEXT
    END
END

